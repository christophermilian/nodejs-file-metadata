{
  "name": "nodejs-file-metadata",
  "version": "1.0.0",
  "description": "A Nodejs full stack app that returns file metadata.",
  "main": "server.js",
  "scripts": {
    "start": "nodemon ./src/server.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "patch-version": "npm --no-git-tag-version version patch",
    "minor-version": "npm --no-git-tag-version version minor",
    "major-version": "npm --no-git-tag-version version major",
    "prepare": "husky install"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christophermilian/nodejs-file-metadata.git"
  },
  "keywords": [
    "express",
    "nodejs",
    "metadata"
  ],
  "author": "Christopher Milian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christophermilian/nodejs-file-metadata/issues"
  },
  "homepage": "https://github.com/christophermilian/nodejs-file-metadata#readme"
}
